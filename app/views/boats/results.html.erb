<div class="row" id="results-wrapper">
  <div class="col-xs-8 results-display" id="results-column">
    <%= render "results" %>
  </div>

  <div class="col-xs-4 results-display" id="results-map">
    <div id="map">
    </div>
  </div>
</div>


<script>
  function initMap() {
    var map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 41.1206, lng: 13.9098},
      scrollwheel: false,
      zoom: 8
    });
  }
</script>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>

    <!-- Gmaps JavaScript -->
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDaExROizZh6pO1qaEN1HNJgyjpFV4LUiY&callback=initMap">
        </script>
