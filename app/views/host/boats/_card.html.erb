<%= link_to boat_path(partial_boat) do %>
   <div class="card">
      <%= cl_image_tag(partial_boat.photos.first.path, class: "card-image", alt: partial_boat.name) %>
      <div class="card-details">
        <div class="title"> <h5> <%= partial_boat.name %> </h5></div>
        <div class="description"><h6> <%= partial_boat.category%> - for <%= partial_boat.capacity%> people </h6> </div>
      </div>
  </div>
<% end %>

